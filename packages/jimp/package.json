{
  "name": "@mugshot/jimp",
  "description": "Custom Jimp instance to use in Mugshot",
  "keywords": ["jimp", "mugshot"],
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiGhTTraX/mugshot.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "homepage": "https://github.com/NiGhTTraX/mugshot/tree/master/packages/jimp",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
