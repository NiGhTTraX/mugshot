{
  "name": "mugshot",
  "publishConfig": {
    "access": "public"
  },
  "version": "3.3.0",
  "description": "Visual regression testing library",
  "keywords": [
    "visual",
    "regression",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiGhTTraX/mugshot.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "homepage": "https://github.com/NiGhTTraX/mugshot/tree/master/packages/mugshot",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn run build",
    "test": "tdd-buffet test --coverage",
    "report-coverage": "codecov -F mugshot"
  },
  "dependencies": {
    "@types/fs-extra": "~9.0.0",
    "@types/is-ci": "~3.0.0",
    "@types/node": "~10.17.0",
    "fs-extra": "~9.1.0",
    "is-ci": "~3.0.0",
    "jimp": "~0.16.0",
    "pixelmatch": "~5.2.0"
  },
  "devDependencies": {
    "@types/pixelmatch": "~5.2.0",
    "puppeteer": "~9.0.0",
    "typescript": "~4.2.0"
  }
}
