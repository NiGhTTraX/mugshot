{
  "name": "@mugshot/webdriverio",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.0-alpha.1",
  "description": "A WebdriverIO adapter for Mugshot",
  "keywords": [
    "mugshot",
    "webdriverio",
    "visual",
    "regression",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiGhTTraX/mugshot.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "homepage": "https://github.com/uberVU/mugshot/tree/master/packages/webdriverio",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "SELENIUM_HOST=0.0.0.0 SELENIUM_PORT=4444 tdd-buffet test --config ../../tests/jest.config.js --coverage --runInBand",
    "pretest": "yarn run selenium",
    "selenium": "selenium start",
    "selenium:debug": "selenium debug",
    "selenium:stop": "selenium stop",
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn run build"
  },
  "peerDependencies": {
    "mugshot": "^2.0.0",
    "webdriverio": "^5.8.3"
  },
  "devDependencies": {
    "@tdd-buffet/selenium": "~0.4.1",
    "mugshot": "^3.0.0-alpha.1",
    "typescript": "~4.0.0",
    "webdriverio": "~5.22.0"
  }
}
