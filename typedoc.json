{
  "out": "./docs",
  "inputFiles": [
    "./packages"
  ],
  "mode": "file",
  "name": "Mugshot",
  "readme": "./DOCS.md",
  "gitRevision": "master",
  "excludeNotExported": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "listInvalidSymbolLinks": true,
  "exclude": [
    "**/tests/**/*"
  ],
  "media": "./media",
  "theme": "pages-plugin",
  "pages": {
    "failBuildOnInvalidPageLink": true,
    "output": "pages",
    "groups": [
      {
        "title": "Adapters",
        "output": "adapters",
        "pages": [
          {
            "title": "WebdriverIO",
            "output": "webdriverio",
            "source": "./packages/webdriverio/README.md"
          },
          {
            "title": "Puppeteer",
            "output": "puppeteer",
            "source": "./packages/puppeteer/README.md"
          },
          {
            "title": "Playwright",
            "output": "playwright",
            "source": "./packages/playwright/README.md"
          }
        ]
      },
      {
        "title": "Contract tests",
        "output": "contracts",
        "pages": [
          {
            "title": "Docs",
            "output": "docs",
            "source": "./packages/contracts/README.md"
          }
        ]
      }
    ]
  }
}
